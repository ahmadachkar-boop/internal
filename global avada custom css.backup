/* ========================================================================== */
/* ========= PAGE-WIDE WAVY BACKGROUND AND STAFF SPOTLIGHT FIX =============== */
/* ========================================================================== */






/* Neutralize Avada wrappers to prevent background override */
/* Remove `.fusion-body` from the grouped selector */
/* Do not constrain the body or full-bleed bands */
.fusion-body { max-width: none !important; margin: 0 !important; width: 100% !important; }

.fusion-body .fusion-fullwidth,
.fusion-body .fusion-footer,
.fusion-body .fusion-page-title-bar {
  max-width: none !important;
  width: 100% !important;
  margin: 0 !important;
}

/* Constrain only inner content */
.fusion-body #main,
.fusion-body .fusion-content-wrapper,
.fusion-body .fusion-row,
.fusion-body .fusion-builder-container,
.fusion-body .fusion-builder-row,
.fusion-body .fusion-column-wrapper,
.fusion-body .fusion-layout-column {
  max-width: var(--shell-max, 1240px) !important;
  margin-left: auto !important;
  margin-right: auto !important;
  width: 100% !important;
  background: transparent !important;
}


/* Ensure inner wrappers are centered and transparent */
.carpool-wrap,
.cta-shell {
  background: transparent !important;
  max-width: var(--shell-max, 1240px) !important;
  margin: 0 auto !important;
  padding-inline: var(--shell-pad, 24px) !important;
  padding-bottom: 0 !important;
  width: 100% !important;
  box-sizing: border-box !important;
}






/* CTA Section - Remove redundant background, align with page */
.fusion-body .carpool-landing .cta-section,
.carpool-landing .cta-section {
  background: transparent !important; /* Inherit page background */
  width: 100% !important;
  max-width: var(--shell-max, 1240px) !important;
  margin: 24px auto 0 !important;
  padding: 24px var(--shell-pad, 24px) 32px !important;
  position: relative !important;
  box-sizing: border-box !important;
}

/* Decorative wave above CTA section */
.fusion-body .carpool-landing .cta-section::before,
.carpool-landing .cta-section::before {
  content: "" !important;
  position: absolute !important;
  top: -40px !important;
  left: 50% !important;
  transform: translateX(-50%) !important;
  width: 100% !important;
  max-width: var(--shell-max, 1240px) !important;
  height: 40px !important;
  background: radial-gradient(60% 100% at 50% 100%, rgba(80,0,0,.14), transparent 70%) !important;
  pointer-events: none !important;
  z-index: -1 !important;
}

/* CTA Inner Container */
.fusion-body .carpool-landing .cta-section .cta-shell,
.carpool-landing .cta-section .cta-shell {
  max-width: var(--shell-max, 1240px) !important;
  margin: 0 auto !important;
  padding: 0 !important;
  width: 100% !important;
  box-sizing: border-box !important;
  background: transparent !important;
}

/* Fix Staff Spotlight Arrows */
.fusion-body .carpool-landing #team-spotlight .ts-controls,
.carpool-landing #team-spotlight .ts-controls {
  display: flex !important;
  gap: 8px !important;
  margin: 12px !important;
  position: relative !important;
  z-index: 10 !important;
  opacity: 1 !important;
  visibility: visible !important;
}

.fusion-body .carpool-landing #team-spotlight .ts-prev,
.fusion-body .carpool-landing #team-spotlight .ts-next,
.carpool-landing #team-spotlight .ts-prev,
.carpool-landing #team-spotlight .ts-next {
  width: 40px !important;
  height: 40px !important;
  border-radius: 10px !important;
  border: 1px solid rgba(17,24,39,.12) !important;
  background: #fff !important;
  font-size: 20px !important;
  line-height: 1 !important;
  box-shadow: 0 6px 16px rgba(0,0,0,.08) !important;
  display: grid !important;
  place-items: center !important;
  cursor: pointer !important;
  opacity: 1 !important;
  pointer-events: auto !important;
}

.fusion-body .carpool-landing #team-spotlight .ts-prev:hover,
.fusion-body .carpool-landing #team-spotlight .ts-next:hover,
.carpool-landing #team-spotlight .ts-prev:hover,
.carpool-landing #team-spotlight .ts-next:hover {
  background: #f3f4f6 !important;
}

.fusion-body .carpool-landing #team-spotlight .ts-stage,
.carpool-landing #team-spotlight .ts-stage {
  overflow: visible !important;
}

/* Mobile adjustments */
@media (max-width: 700px) {
  body.carpool-landing,
  .fusion-body .carpool-landing {
    background: var(--cta-bg) !important;
    background-size: var(--cta-bg-size) !important;
    background-position: var(--cta-bg-position) !important;
    background-repeat: var(--cta-bg-repeat) !important;
  }

  .fusion-body .carpool-landing .cta-section,
body.home .carpool-landing .cta-section {
    padding: 16px var(--shell-pad, 12px) 24px !important;
    margin: 16px auto 0 !important;
    width: 100% !important;
    max-width: var(--shell-max, 1240px) !important;
    background: transparent !important;
  }

  .fusion-body .carpool-landing .cta-section::before,
body.home .carpool-landing .cta-section::before {
    width: 100% !important;
    max-width: none !important;
    left: 0 !important;
    transform: none !important;
  }

  .fusion-body .carpool-landing .cta-section .cta-shell,
body.home .carpool-landing .cta-section .cta-shell {
    padding: 0 !important;
    max-width: none !important;
  }

  .carpool-wrap,
  .cta-shell {
    padding-inline: max(12px, env(safe-area-inset-left)) !important;
  }

  .fusion-body .carpool-landing #team-spotlight .ts-controls,
body.home .carpool-landing #team-spotlight .ts-controls {
    margin: 8px !important;
  }

  .fusion-body .carpool-landing #team-spotlight .ts-prev,
  .fusion-body .carpool-landing #team-spotlight .ts-next,
body.home .carpool-landing #team-spotlight .ts-prev,
body.home .carpool-landing #team-spotlight .ts-next {
    width: 36px !important;
    height: 36px !important;
    font-size: 18px !important;
    border-radius: 9px !important;
  }
}

/* Ultra-narrow screens */
@media (max-width: 360px) {
  body.carpool-landing,
  .fusion-body .carpool-landing {
    padding: 12px var(--shell-pad, 10px) 20px !important;
  }

  .fusion-body .carpool-landing .cta-section,
body.home .carpool-landing .cta-section {
    padding: 12px var(--shell-pad, 10px) 20px !important;
  }
}












/* CTA Section - Centered, aligned with other sections, no right-side cutoff */
.cta-section {
  width: 100%;
  max-width: var(--shell-max, 1240px); /* Match carpool-wrap max-width */
  margin: 24px auto 0; /* Center horizontally, match other sections */
  padding: 24px var(--shell-pad, 24px) 32px; /* Symmetric padding */
  position: relative;
  box-sizing: border-box;
  background: var(--cta-bg); /* Wavy conic gradient */
  background-size: var(--cta-bg-size);
  background-position: var(--cta-bg-position);
  background-repeat: var(--cta-bg-repeat);
}

/* Decorative wave above, full-width but visually centered */
.cta-section::before {
  content: "";
  position: absolute;
  top: -40px;
  left: 50%;
  transform: translateX(-50%);
  width: 100%; /* Full viewport for background */
  max-width: var(--shell-max, 1240px); /* Constrain to content width */
  height: 40px;
  background: radial-gradient(60% 100% at 50% 100%, rgba(80,0,0,.14), transparent 70%);
  pointer-events: none;
  z-index: -1;
}

/* Inner container - Matches carpool-wrap alignment */
.cta-shell {
  max-width: var(--shell-max, 1240px); /* Same as carpool-wrap */
  margin: 0 auto; /* Center within section */
  padding: 0; /* No extra padding, rely on cta-section */
  width: 100%;
  box-sizing: border-box;
}

/* Header - Centered text */
.cta-head {
  text-align: center;
  margin: 0 0 14px;
}

.cta-head .eyebrow {
  display: inline-block;
  margin: 0 0 6px;
  padding: 6px 10px;
  font-weight: 800;
  letter-spacing: .2px;
  font-size: .9rem;
  border: 1px solid rgba(17,24,39,.12);
  border-radius: 999px;
  background: #fff;
}

.cta-head h2 {
  margin: 0;
  font-weight: 900;
  letter-spacing: .2px;
  font-size: clamp(20px, 2.4vw, 28px);
  color: var(--text-heading, #0a0a0a);
  text-decoration: none !important;
}

.cta-head .sub {
  margin: 6px 0 0;
  color: var(--text-secondary, #4b5563);
  font-size: clamp(0.95rem, 1.8vw, 1rem);
}

/* CTA Grid - Three columns on desktop, one on mobile */
.cta-grid {
  list-style: none;
  margin: 14px 0 0;
  padding: 0;
  display: grid;
  grid-template-columns: repeat(3, minmax(0, 1fr));
  gap: 12px;
  width: 100%;
  max-width: 100%;
}

.cta-grid > :not(.tile) {
  display: none !important;
}

.cta-grid > .tile > :not(.cta-tile) {
  display: none !important;
}

.cta-grid > .tile {
  margin: 0;
  padding: 0;
}

/* CTA Tiles - Uniform, no overflow */
.cta-tile {
  display: flex;
  align-items: center;
  gap: 12px;
  text-decoration: none;
  border: 1px solid rgba(17,24,39,.08);
  border-radius: 16px;
  padding: 14px;
  background: #fff;
  color: #111;
  box-shadow: 0 16px 40px -22px rgba(0,0,0,.28);
  transition: transform .16s ease, box-shadow .2s ease, border-color .2s ease;
  width: 100%;
  box-sizing: border-box;
  min-width: 0;
}

.cta-tile:hover {
  transform: translateY(-2px);
  box-shadow: 0 22px 54px -26px rgba(0,0,0,.32);
  border-color: rgba(17,24,39,.14);
}

.cta-primary,
.cta-accent,
.cta-neutral {
  background: #fff;
  border: 1px solid rgba(17,24,39,.08);
}

.cta-primary:hover,
.cta-accent:hover,
.cta-neutral:hover {
  border-color: rgba(17,24,39,.14);
}

/* CTA Icon - Centered, consistent size */
.cta-icon {
  width: 44px;
  height: 44px;
  border-radius: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
  border: 1px solid rgba(17,24,39,.10);
  box-shadow: 0 8px 18px rgba(0,0,0,.08);
  color: var(--accent, #500000);
  flex: 0 0 44px;
  line-height: 0;
}

.cta-icon svg {
  width: 22px;
  height: 22px;
  display: block;
}

/* CTA Copy - Prevent text overflow */
.cta-copy {
  flex: 1;
  min-width: 0;
}

.cta-copy strong {
  display: block;
  font-weight: 900;
  font-size: 1rem;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.cta-copy .meta {
  display: block;
  color: var(--text-secondary, #4b5563);
  font-weight: 700;
  font-size: .95rem;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

/* CTA Support Toolbar - Aligned with content */
.cta-support {
  display: flex;
  align-items: center;
  gap: 8px;
  flex-wrap: wrap;
  margin-top: 12px;
  background: rgba(255,255,255,.8);
  border: 1px solid rgba(17,24,39,.08);
  border-radius: 12px;
  padding: 10px 12px;
  box-shadow: 0 12px 34px -22px rgba(0,0,0,.24);
  width: 100%;
  box-sizing: border-box;
}

.cta-support .chip {
  display: inline-flex;
  align-items: center;
  gap: 8px;
  padding: 8px 12px;
  border-radius: 999px;
  background: #fff;
  border: 1px solid rgba(17,24,39,.12);
  font-weight: 800;
  color: #111;
  text-decoration: none;
  min-height: 44px;
}

.cta-support .chip:hover {
  background: #f3f4f6;
}

.cta-support .spacer {
  flex: 1 1 auto;
}

.cta-social {
  display: flex;
  gap: 8px;
}

.cta-social .social {
  display: grid;
  place-items: center;
  width: 38px;
  height: 38px;
  border-radius: 10px;
  text-decoration: none;
  background: #fff;
  border: 1px solid rgba(17,24,39,.12);
  box-shadow: 0 8px 18px rgba(0,0,0,.06);
  font-weight: 900;
  color: #111;
}

.cta-social .social:hover {
  background: #f3f4f6;
}

/* Focus states for accessibility */
.cta-tile:focus-visible,
.cta-support .chip:focus-visible,
.cta-social .social:focus-visible {
  outline: 3px solid var(--ring, color-mix(in srgb, var(--accent, #500000) 40%, #ffffff));
  outline-offset: 2px;
  border-radius: 12px;
}

/* Avada-specific overrides - Ensure no extra margins/padding */
.fusion-body .cta-section,
.fusion-body .cta-section .fusion-row,
.fusion-body .cta-section .fusion-builder-row,
.fusion-body .cta-section .fusion-column-wrapper,
.fusion-body .cta-section .fusion-layout-column {
  margin: 0 !important;
  padding: 0 !important;
  width: 100% !important;
  max-width: var(--shell-max, 1240px) !important;
  margin-left: auto !important;
  margin-right: auto !important;
}

.fusion-body .cta-section .cta-shell {
  max-width: var(--shell-max, 1240px) !important;
  margin: 0 auto !important;
  padding: 0 !important;
}

/* Mobile adjustments - Align with other sections */
@media (max-width: 700px) {
  .cta-section {
    padding: 16px var(--shell-pad, 12px) 24px;
    margin: 16px auto 0;
    width: 100%;
    max-width: var(--shell-max, 1240px);
  }

  .cta-shell {
    padding: 0;
    max-width: none;
  }

  .cta-section::before {
    width: 100%; /* Match section width */
    max-width: none;
    left: 0;
    transform: none;
  }

  .cta-grid {
    gap: 8px;
    margin: 10px 0 0;
    grid-template-columns: 1fr;
  }

  .cta-tile {
    padding: 10px;
    border-radius: 10px;
    box-shadow: 0 6px 16px -10px rgba(0,0,0,0.2);
    width: 100%;
    min-width: 0;
  }

  .cta-icon {
    width: 36px;
    height: 36px;
    border-radius: 8px;
    flex: 0 0 36px;
  }

  .cta-icon svg {
    width: 18px;
    height: 18px;
  }

  .cta-copy strong,
  .cta-copy .meta {
    white-space: normal;
    overflow-wrap: anywhere;
    text-overflow: clip;
    font-size: .9rem;
  }

  .cta-support.bleed {
    margin: 10px calc(-1 * var(--shell-pad, 12px)) 0;
    width: calc(100% + 2 * var(--shell-pad, 12px));
    padding: 10px var(--shell-pad, 12px) calc(10px + env(safe-area-inset-bottom, 0));
    border-radius: 0;
    background: rgba(255,255,255,.85);
    backdrop-filter: saturate(115%) blur(8px);
    -webkit-backdrop-filter: saturate(115%) blur(8px);
    box-shadow: 0 6px 20px -16px rgba(0,0,0,.3);
    position: sticky;
    bottom: 0;
    z-index: 40;
  }

  .cta-support .chip-tray {
    display: flex;
    gap: 6px;
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
    scroll-snap-type: x proximity;
    padding-bottom: 2px;
    width: 100%;
    justify-content: flex-start;
  }

  .cta-support .chip-tray::-webkit-scrollbar {
    display: none;
  }

  .cta-support .chip {
    flex: 0 0 auto;
    min-height: 44px;
    padding: 8px 12px;
    font-size: .9rem;
    scroll-snap-align: start;
  }

  .cta-social {
    gap: 6px;
    flex: 0 0 auto;
  }

  .cta-social .social {
    width: 44px;
    height: 44px;
    border-radius: 10px;
  }

  .cta-support .spacer {
    display: none;
  }
}

/* Ultra-narrow screens - Tighter padding */
@media (max-width: 360px) {
  .cta-section {
    padding: 12px var(--shell-pad, 10px) 20px;
  }
  .cta-support.bleed {
    margin: 8px calc(-1 * var(--shell-pad, 10px)) 0;
    width: calc(100% + 2 * var(--shell-pad, 10px));
    padding: 8px var(--shell-pad, 10px) calc(8px + env(safe-area-inset-bottom, 0));
  }
}






























