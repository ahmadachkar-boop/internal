<script>
  (function() {
    // Store scroll position before opening modal
    let savedScrollPosition = 0;
    
    // Save scroll position when clicking on staff cards
    document.querySelectorAll('.card, .name a').forEach(function(element) {
      element.addEventListener('click', function(e) {
        // Only save if clicking a link that opens a modal
        const href = e.target.closest('a')?.getAttribute('href');
        if (href && href.startsWith('#sheet-')) {
          savedScrollPosition = window.pageYOffset || document.documentElement.scrollTop;
        }
      });
    });
    
    // Restore scroll position when modal is closed
    window.addEventListener('hashchange', function() {
      if (window.location.hash === '' || window.location.hash === '#close') {
        // Small delay to ensure DOM is updated
        setTimeout(function() {
          window.scrollTo(0, savedScrollPosition);
        }, 0);
      }
    });
    
    // Handle overlay and close button clicks
    document.querySelectorAll('.sheet-overlay, .sheet-close').forEach(function(element) {
      element.addEventListener('click', function(e) {
        e.preventDefault();
        window.location.hash = '';
        // Restore scroll position
        setTimeout(function() {
          window.scrollTo(0, savedScrollPosition);
        }, 0);
      });
    });
    
    // Prevent body scroll when modal is open
    const observer = new MutationObserver(function() {
      const hasOpenModal = document.querySelector('.staff-sheet:target');
      document.body.style.overflow = hasOpenModal ? 'hidden' : '';
    });
    
    observer.observe(document.body, { 
      attributes: false, 
      childList: false, 
      subtree: false 
    });
    
    // Check on page load if there's a hash
    if (window.location.hash && window.location.hash.startsWith('#sheet-')) {
      savedScrollPosition = window.pageYOffset || document.documentElement.scrollTop;
    }
  })();
</script>
</section>
</main>

